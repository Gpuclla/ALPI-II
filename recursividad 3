#include <stdio.h>

long factorial(int numero);

int main()
{
    int numero;
    printf("ingrese el numero:");
    scanf("%i",&numero);
    printf("\n el factotrial del numero es %li",factorial(numero));
    return 0;
    }

    long factorial(int n ){
    if(n<=1){
        return 1;
    }
    else {
      return(n*factorial(n-1));
    }
}
